{
    "name": "alltvex/laravel-scout-opensearch",
    "description": "Search among multiple models with ElasticSearch and Laravel Scout",
    "keywords": [
        "laravel",
        "scout",
        "extended",
        "search",
        "opensearch"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Aschan",
            "email": "marcus@autovex.fi"
        }
    ],
    "require": {
        "php": "^8.0.12|^8.1",
        "laravel/scout": "^8.0|^9.0",
        "opensearch-project/opensearch-php": "^2.0",
        "roave/better-reflection": "^4.3|^5.0",
        "shyim/opensearch-php-dsl": "^1.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0",
        "nunomaduro/larastan": "~0.6",
        "orchestra/testbench": "^6.18",
        "php-http/guzzle7-adapter": "^1.0",
        "phpunit/phpunit": "~9.4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "tests/laravel/app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Alltvex\\ScoutOpenSearch\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alltvex\\ScoutOpenSearch\\ScoutOpenSearchServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
